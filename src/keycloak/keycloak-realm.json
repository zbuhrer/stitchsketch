{
  "id": "stitchsketch",
  "realm": "StitchSketch Realm",
  "enabled": true,
  "sslRequired": false,
  "passwordPolicy": "default",
  "credentials": {
    "actions": [
      {
        "type": "bcrypt"
      }
    ]
  },
  "requiredCredentials": [{ "user": { "password": {} } }],
  "loginTheme": "keycloak",
  "accountTheme": "keycloak",
  "adminTheme": "keycloak",
  "userProfileTheme": "keycloak",
  "rememberMeCookiePolicy": {
    "enabled": true,
    "cookieName": "_kdc"
  },
  "clientScopeMappings": {
    "stitchsketch-client": {}
  }
}
